<column-mc:>

  <div style="width:100%">
  <h1>Pages (<%= @pages_count %>)</h1>
    </div>      <br/> <br/> <br/>
  <%= will_paginate @pages %>
  <table class="generic" cellspacing="0" cellpadding="0">
    <%= table_headers "#", "DBId", "Address", "Title", "Number of links", "Crawler", " " %>
    <tbody id="pages_list">

    <% @pages.each_with_index do |page, index| %>
        <%= render :partial => "page", :locals => {:page => page, :index => index} %>
    <% end %>

    </tbody>
  </table>
  <%= will_paginate @pages %>


</column-mc:>